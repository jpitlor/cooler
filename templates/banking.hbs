<div class="container">
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">Account Information</h5>
            <ul class="list-group">
                {{#each info}}
                    <li class="list-group-item">
                        <strong>{{@key}}: </strong>{{this}}
                    </li>
                {{/each}}
            </ul>
            <a href="#" class="btn btn-raised  btn-secondary">Generate Account Statement</a>
        </div>
    </div>

    <div class="card">
        <div class="card-body">
            <h5 class="card-title">Payments Being Processed</h5>
            <table class="table">
                <thead class="thead-light">
                    {{#each paymentsIP.headers}}
                        <th scope="col">{{this}}</th>
                    {{/each}}
                </thead>
                <tbody>
                    {{#each paymentsIP.content}}
                        <tr>
                            {{#each this}}
                                <td>{{this}}</td>
                            {{/each}}
                        </tr>
                    {{/each}}
                </tbody>
            </table>
            <div class="btn-toolbar" role="toolbar" aria-label="Buttons">
                <div class="btn-group mr-2" role="group" aria-label="Add">
                    <button type="button" class="btn btn-raised  btn-secondary">New</button>
                </div>
                <div class="btn-group mr-2" role="group" aria-label="Export">
                    <button type="button" class="btn btn-raised  btn-secondary">Export to PDF</button>
                    <button type="button" class="btn btn-raised  btn-secondary">Microsoft Word Report</button>
                    <button type="button" class="btn btn-raised  btn-secondary">Export to Excel</button>
                    <button type="button" class="btn btn-raised  btn-secondary">Export to CSV</button>
                </div>
                <div class="btn-group" role="group" aria-label="View">
                    <button type="button" class="btn btn-raised  btn-secondary">Refresh</button>
                    <button type="button" class="btn btn-raised  btn-secondary">Clear Filters</button>
                </div>
            </div>
        </div>
    </div>

