<div class="container">
	<div class="row">
		<div class="card col-md mx-3">
			<div class="card-body">
				<h5 class="card-title">Account Information</h5>
				<ul class="list-group">
					{{#each info}}
						<li class="list-group-item">
							<strong>{{@key}}: </strong>{{this}}
						</li>
					{{/each}}
				</ul>
				<a href="#" class="btn btn-outline btn-secondary">Generate Account Statement</a>
			</div>
		</div>

		<div class="card col-md mx-3">
			<div class="card-body">
				<h5 class="card-title">Payments Being Processed</h5>
				<div class="btn-toolbar" role="toolbar" aria-label="Buttons">
					<div class="btn-group mr-2" role="group" aria-label="Add" id="add-button">
						<button type="button" class="btn bmd-btn-icon">
							<i class="fal fa-plus"></i>
						</button>
					</div>
					<div class="btn-group mr-2" role="group" aria-label="Export">
						<button type="button" class="btn bmd-btn-icon">
							<i class="fal fa-file-pdf"></i>
						</button>
						<button type="button" class="btn bmd-btn-icon">
							<i class="fal fa-file-word"></i>
						</button>
						<button type="button" class="btn bmd-btn-icon">
							<i class="fal fa-file-excel"></i>
						</button>
						<button type="button" class="btn bmd-btn-icon">
							<i class="fal fa-file-alt"></i>
						</button>
					</div>
					<div class="btn-group mr-2" role="group" aria-label="Add">
						<button type="button" class="btn bmd-btn-icon">
							<i class="fal fa-sync"></i>
						</button>
						<button type="button" class="btn bmd-btn-icon">
							<i class="fal fa-filter"></i>
						</button>
					</div>
				</div>
				<table class="table table-hover">
					<thead class="thead-light">
						{{#each paymentsIP.headers}}
							<th scope="col">{{this}}</th>
						{{/each}}
					</thead>
					<tbody>
						{{#each paymentsIP.content}}
							<tr>
								{{#each this}}
									<td>{{this}}</td>
								{{/each}}
							</tr>
						{{/each}}
					</tbody>
				</table>
			</div>
		</div>
	</div>

	<div class="card">
		<div class="card-body">
			<h5 class="card-title">Completed Payments</h5>
			<div class="btn-toolbar" role="toolbar" aria-label="Buttons">
                <button type="button" class="btn bmd-btn-icon" style="display: none">
                    <!-- Placeholder to keep indexes correct -->
                </button>
                <div class="btn-group mr-2" role="group" aria-label="Export">
                    <button type="button" class="btn bmd-btn-icon">
						<i class="fal fa-file-pdf"></i>
					</button>
					<button type="button" class="btn bmd-btn-icon">
						<i class="fal fa-file-word"></i>
					</button>
					<button type="button" class="btn bmd-btn-icon">
						<i class="fal fa-file-excel"></i>
					</button>
					<button type="button" class="btn bmd-btn-icon">
						<i class="fal fa-file-alt"></i>
					</button>
				</div>
				<div class="btn-group mr-2" role="group" aria-label="Add">
					<button type="button" class="btn bmd-btn-icon">
						<i class="fal fa-sync"></i>
					</button>
					<button type="button" class="btn bmd-btn-icon">
						<i class="fal fa-filter"></i>
					</button>
				</div>
			</div>
			<table class="table table-hover">
				<thead class="thead-light">
					{{#each payments.headers}}
						<th scope="col">{{this}}</th>
					{{/each}}
				</thead>
				<tbody>
					{{#each payments.content}}
						<tr>
							{{#each this}}
								<td>{{this}}</td>
							{{/each}}
						</tr>
					{{/each}}
				</tbody>
			</table>
		</div>
	</div>

