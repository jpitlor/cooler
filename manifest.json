{
	"name": "COOLer",
	"description": "A COOL extension",
	"version": "1.0",
	"manifest_version": 2,
	"web_accessible_resources": [
		"templates/*",
		"theme.hbs"
	],
	"content_scripts": [
		{
			"css": ["vendor/bootstrap.min.css", "theme.css"],
			"js": ["vendor/jquery.min.js", "vendor/popper.min.js", "vendor/bootstrap.min.js", "vendor/icons.min.js",
				"vendor/handlebars.js", "theme.js"],
			"matches": ["https://www.coolfaces.net/*"]
		},
		{
			"js": ["pages/browser-support.js"],
			"matches": ["https://www.coolfaces.net/COOLPUWL/MyPages/Browsers.html"]
		},
		{
			"js": ["pages/home.js"],
			"matches": ["https://www.coolfaces.net/COOLPUWL/Individual/ShowIndividualPage.aspx*"]
		},
		{
			"js": ["pages/edit-individual.js"],
			"matches": ["https://www.coolfaces.net/COOLPUWL/Individual/EditIndividualPage.aspx*"]
		},
		{
			"js": ["pages/club.js"],
			"matches": ["https://www.coolfaces.net/COOLPUWL/StudentOrganization/ShowStudentOrganizationWVendorPage.aspx*"]
		},
		{
			"js": ["pages/banking.js"],
			"matches": ["https://www.coolfaces.net/COOLPUWL/StudentOrganizationAccounts/ShowStudentOrganizationAccountsWActWVendorPage.aspx*"]
		}
	]
}