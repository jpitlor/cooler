{
	"name": "COOLer",
	"description": "A COOL extension",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts": [
		{
			"css": ["vendor/bootstrap.min.css", "theme.css"],
			"js": ["vendor/jquery.min.js", "vendor/popper.min.js", "vendor/bootstrap.min.js", "theme.js"],
			"matches": ["https://www.coolfaces.net/*"]
		},
		{
			"js": ["pages/browser-support.js"],
			"matches": ["https://www.coolfaces.net/COOLPUWL/MyPages/Browsers.html"]
		},
		{
			"js": ["pages/home.js"],
			"matches": ["https://www.coolfaces.net/COOLPUWL/Individual/ShowIndividualPage.aspx*"]
		}
	]
}